{
  "manifest_version": 3,
  "name": "Magister Auto Login",
  "version": "1.0.0",
  "description": "Automatically Logs in Magister",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "host_permissions": [
    "*://*.magister.net/*",
    "*://sso.ssc.college/*",
    "*://login.microsoftonline.com/*",
    "*://*.itslearning.com/*"
  ],
  "action": {
    "default_title": "Auto-login",
    "default_popup": "/popup/index.html",
    "default_icon": {
      "16": "icons/16x.png",
      "32": "icons/32x.png",
      "48": "icons/48x.png",
      "128": "icons/128x.png",
      "256": "icons/256x.png"
    }
  },
  "icons": {
    "16": "icons/16x.png",
    "32": "icons/32x.png",
    "48": "icons/48x.png",
    "128": "icons/128x.png",
    "256": "icons/256x.png"
  },
  "background": {
    "service_worker": "js/background.js"
  }
}

